{
  "legacyEditor": false,
  "livePreview": true,
  "alwaysUpdateLinks": true,
  "strictLineBreaks": false,
  "showFrontmatter": true,
  "fileSortOrder": "alphabetical",
  "pdfExportSettings": {
    "pageSize": "Letter",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 90
  },
  "mobilePullAction": "switcher:open",
  "mobileToolbarCommands": [
    "editor:toggle-keyboard",
    "obsidian-excalidraw-plugin:excalidraw-autocreate-and-embed-on-current",
    "editor:undo",
    "editor:redo",
    "editor:toggle-checklist-status",
    "editor:insert-tag",
    "editor:go-start",
    "editor:go-end",
    "editor:move-caret-up",
    "editor:move-caret-down",
    "editor:move-caret-left",
    "editor:move-caret-right",
    "editor:insert-wikilink",
    "editor:insert-embed",
    "editor:attach-file",
    "editor:indent-list",
    "editor:unindent-list",
    "editor:context-menu",
    "editor:configure-toolbar"
  ],
  "showUnsupportedFiles": true,
  "attachmentFolderPath": "Assets",
  "newFileLocation": "current",
  "showInlineTitle": false,
  "vimMode": false,
  "useMarkdownLinks": true,
  "newLinkFormat": "absolute"
}